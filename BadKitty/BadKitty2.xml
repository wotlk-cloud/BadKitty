	<!--
	
			Cat Warnings
		
		
	-->
	
	<Frame name="BadKittyWarnings" movable="true" parent="UIParent" toplevel="true">
		<Size>
			<AbsDimension x="150" y="150" />
		</Size>
		
		<Anchors>
			<Anchor point ="CENTER"/>
		</Anchors>
		<Scripts>
		<OnMouseDown>
			BKWMove()
		</OnMouseDown>
		<OnMouseUp>
			BadKittyWarnings:StopMovingOrSizing()
		</OnMouseUp>
		<OnDragStop>
			BadKittyWarnings:StopMovingOrSizing()
		</OnDragStop>
		<OnShow>
			BadKitty_CatWarningShape(BadKittySaves[14])
		</OnShow>
